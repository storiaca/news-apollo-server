{
  "name": "news-apollo-server",
  "version": "1.0.0",
  "description": "Graphql apollo2 server",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "nodemon --exec babel-node server.js"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 9,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storiaca/news-apollo-server.git"
  },
  "keywords": [
    "graphql",
    "apollo",
    "server"
  ],
  "author": "Aleksandar Ristic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/storiaca/news-apollo-server/issues"
  },
  "homepage": "https://github.com/storiaca/news-apollo-server#readme",
  "dependencies": {
    "apollo-datasource-rest": "^0.6.9",
    "apollo-server": "^2.25.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "apollo-server-testing": "^2.9.13",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2"
  }
}
